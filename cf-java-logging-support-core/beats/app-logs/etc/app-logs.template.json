          "index": "not_analyzed",
          "type": "string"
        },
        "component_name": {
          "doc_values": true,
          "index": "not_analyzed",
          "type": "string"
        },
        "component_type": {
          "doc_values": true,
          "index": "not_analyzed",
          "type": "string"
        },
        "container_id": {
          "doc_values": true,
          "index": "not_analyzed",
          "type": "string"
        },
        "correlation_id": {
          "doc_values": true,
          "index": "not_analyzed",
          "type": "string"
        },
        "layer": {
          "doc_values": true,
          "index": "not_analyzed",
          "type": "string"
        },
        "level": {
          "doc_values": true,
          "index": "not_analyzed",
          "type": "string"
        },
        "logger": {
          "doc_values": true,
          "index": "not_analyzed",
